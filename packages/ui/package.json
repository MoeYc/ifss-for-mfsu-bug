{
  "name": "@ifss/ui",
  "version": "0.0.1",
  "author": "buq",
  "repository": {
    "type": "git",
    "url": "https://github.com/buquan/ifss"
  },
  "scripts": {
    "dev": "father-build --watch",
    "docdev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "build": "father-build",
    "prepublishOnly": "npm run build && cp -r ./es/* . && rm -rf ./es",
    "release": "npm publish",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "format": "prettier --write \"**/*.{js,jsx,tsx,ts,less,json}\"",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix '**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@ifss/tools": "workspace:*"
  },
  "devDependencies": {
    "@ifss/config": "workspace:*",
    "@ifss/eslint-config-custom": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "es",
    "lib",
    "index.d.ts"
  ],
  "module": "./es/index.js",
  "main": "./lib/index.js",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "sideEffects": [
    "./src/**/*.less"
  ]
}
